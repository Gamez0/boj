    "a, b = input().split()\n",
    "b=list(b)#b가 list형태여야 del를 사용할 수 있다"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 57,
   "metadata": {},
   "outputs": [],
   "source": [
    "ans =0\n",
    "while len(a)<=len(b):\n",
    "    cnt=0\n",
    "    for i in range(len(a)):\n",
    "        if a[i]==b[i]:\n",
    "            cnt+=1\n",
    "    ans = max(ans,cnt)\n",
    "    del b[0]  # b의 앞 원소를 제거해줌으로써"
   ]
  },
